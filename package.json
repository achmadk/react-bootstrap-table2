{
  "name": "react-bootstrap-table2",
  "version": "0.0.1",
  "private": true,
  "description": "Rebuilt for react-bootstrap-table",
  "main": "index.js",
  "scripts": {
    "postinstall": "lerna link --force-local",
    "package-check": "cross-env NODE_ENV=production lerna exec --ignore react-bootstrap-table2-example package-check",
    "build:js": "preconstruct build",
    "build:css:dev": "lerna exec --ignore react-bootstrap-table2-{editor-esm,example,overlay-esm} -- rollup -c",
    "build:css:prod": "cross-env NODE_ENV=production lerna exec --ignore react-bootstrap-table2-{editor-esm,example,overlay-esm} -- rollup -c",
    "build:remove-dist-css-index": "cross-env NODE_ENV=production lerna exec --ignore react-bootstrap-table2-{editor-esm,example,overlay-esm} -- rimraf ./dist/css/index.js",
    "build": "yarn run build:js && yarn run build:css:dev && yarn run build:css:prod && yarn run build:remove-dist-css-index",
    "legacy-build": "./node_modules/.bin/gulp prod",
    "check:dts": "cross-env NODE_ENV=production lerna exec --ignore react-bootstrap-table2-example -- dtslint types",
    "lint": "eslint ./packages --ext .js --ext .jsx --ignore-path .gitignore",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules lerna exec -- jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "storybook": "cd ./packages/react-bootstrap-table2-example && yarn storybook",
    "gh-pages:clean": "cd ./packages/react-bootstrap-table2-example && yarn gh-pages:clean",
    "gh-pages:build": "cd ./packages/react-bootstrap-table2-example && yarn gh-pages:build",
    "release": "yarn install && yarn build && lerna publish",
    "prepublishOnly": "yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-bootstrap-table/react-bootstrap-table2.git"
  },
  "author": "AllenFang",
  "contributors": [
    {
      "name": "Allen Fang",
      "email": "ayu780129@hotmail.com",
      "url": "https://github.com/AllenFang"
    },
    {
      "name": "Chun-MingChen",
      "email": "nick830314@gmail.com",
      "url": "https://github.com/Chun-MingChen"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/react-bootstrap-table/react-bootstrap-table2/issues"
  },
  "homepage": "https://github.com/react-bootstrap-table/react-bootstrap-table2#readme",
  "devDependencies": {
    "@babel/cli": "7.12.13",
    "@babel/core": "7.12.13",
    "@babel/plugin-proposal-class-properties": "7.12.13",
    "@babel/plugin-proposal-function-bind": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@babel/preset-react": "7.12.13",
    "@babel/register": "7.12.13",
    "@preconstruct/cli": "2.0.4",
    "@skypack/package-check": "0.2.2",
    "@storybook/cli": "6.1.17",
    "@types/file-saver": "2.0.1",
    "@types/react": "17.0.1",
    "@types/react-bootstrap-table-next": "4.0.10",
    "@types/react-bootstrap-table2-filter": "1.3.1",
    "@types/react-bootstrap-table2-paginator": "2.1.1",
    "@types/react-bootstrap-table2-toolkit": "2.1.4",
    "@types/webpack": "4.41.26",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-loader": "8.2.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "cross-env": "7.0.3",
    "css-loader": "5.0.2",
    "dtslint": "4.0.7",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16.3": "1.7.4",
    "enzyme-to-json": "3.6.1",
    "eslint": "7.19.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-clean": "0.4.0",
    "gulp-clean-css": "4.3.0",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "4.1.0",
    "gulp-shell": "0.8.0",
    "html-webpack-plugin": "5.0.0",
    "jest": "26.6.3",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "lerna": "3.22.1",
    "node-sass": "5.0.0",
    "react-test-renderer": "17.0.1",
    "rollup-plugin-scss": "2.6.1",
    "sass": "1.32.7",
    "sass-loader": "11.0.1",
    "sinon": "9.2.4",
    "style-loader": "2.0.0",
    "typescript": "4.1.5",
    "webpack": "5.21.2",
    "webpack-dev-server": "3.11.2"
  },
  "dependencies": {
    "@types/events": "3.0.0",
    "classnames": "^2.2.6",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-loading-overlay-ts": "0.1.0",
    "underscore": "1.12.0"
  },
  "workspaces": [
    "packages/react-bootstrap-table2",
    "packages/react-bootstrap-table2-editor",
    "packages/react-bootstrap-table2-filter",
    "packages/react-bootstrap-table2-overlay",
    "packages/react-bootstrap-table2-paginator",
    "packages/react-bootstrap-table2-toolkit"
  ],
  "jest": {
    "collectCoverageFrom": [
      "packages/*/src/**/*.js",
      "packages/*/index.js",
      "!packages/react-bootstrap-table2-example/**/*.js"
    ],
    "roots": [
      "<rootDir>/packages"
    ],
    "setupFiles": [
      "<rootDir>/enzyme-setup.js"
    ],
    "modulePaths": [
      "<rootDir>/packages/react-bootstrap-table2"
    ],
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.js"
    ]
  },
  "preconstruct": {
    "packages": [
      "packages/react-bootstrap-table2",
      "packages/react-bootstrap-table2-editor",
      "packages/react-bootstrap-table2-filter",
      "packages/react-bootstrap-table2-overlay",
      "packages/react-bootstrap-table2-paginator",
      "packages/react-bootstrap-table2-toolkit"
    ]
  }
}
